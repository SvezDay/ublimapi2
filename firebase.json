{
  "hosting": {
    "public": "public",
    "rewrites":[{
      "source":"**",
      "function": "app"
      }],
    "headers": [ {
      "source" : "**",
      "headers" : [{
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      }]
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
